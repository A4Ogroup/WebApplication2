                            <div class="col-12">
                                <div class="mb-5">
                                    <h2>Log in</h2>
                                </div>
                            </div>
                        </div>
                        <form action="#!">
                            <div class="row gy-3 overflow-hidden">
                                <div class="col-12">
                                    <div class="form-floating mb-1">
                                        <input type="email" class="form-control" name="email" id="email" placeholder="name@example.com" required>
                                        <label for="email" class="form-label">Email</label>
                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="form-floating mb-1">
                                        <input type="password" class="form-control" name="password" id="password" value="" placeholder="Password" required>
                                        <label for="password" class="form-label">Password</label>
                                    </div>
                                </div>
                                <div class="col-12">

                                    <div class="d-flex  justify-content-md-start">
                                        <a class="link-secondary btn btn-light">Forgot password</a>

                                    </div>
                                </div>
                                <div class="col-12">
                                    <div class="d-grid">
                                        <button class="btn bsb-btn-2xl btn-primary filled-button" type="submit">Log in</button>
                                    </div>
                                </div>
                            </div>
                        </form>

                        <div class="row">
                            <div class="col-12">
                                <p class="mt-5 mb-4">Or continue with</p>
                                <div class="d-flex gap-3 flex-column">
                                    <a href="#!" class="btn bsb-btn-xl btn-outline-danger btn-light" style="color:white; background-color: red;">
                                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-google" viewBox="0 0 16 16">
