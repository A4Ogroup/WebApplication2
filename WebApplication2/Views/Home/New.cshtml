                                        <li><i class="fa fa-star"></i></li>
                                        <li><i class="fa fa-star"></i></li>
                                        <li><i class="fa fa-star"></i></li>

                                        break;
                                    case 2:
                                        <li><i class="fa fa-star"></i></li>
                                        <li><i class="fa fa-star"></i></li>
                                        <li><i class="fa fa-star"></i></li>

                                        break;
                                    default:
                                        <li><i class="fa fa-star"></i></li>
                                        break;
                                }


                            </ul>
                            <p>@items.Descritipn</p>
                        </div>
                    </div>
                </div>
            }

        </div>
    </div>
</div> 




   
